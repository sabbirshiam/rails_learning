<h1>Agents#index</h1>
<p>
	<%= link_to 'Create New', new_agent_path %>
</p>
<table>
  <tr>
    <th>Name</th>
    <th>image</th>
    <th>status</th>
    <th>Edit/delete</th>
  </tr>
 
  <% @agents.each do |agent| %>
    <tr>
      <td><%= agent.name %></td>
      <td><img src="<%= show_image_agent_path(agent)%>" width= 60px, height=60px, style="border-radius: 50%"/></td>
      <td><%= agent.disabled_at.nil? ? Agent.human_enum_name(:status, Agent.statuses.key(1).to_s) : 
      Agent.human_enum_name(:status, Agent.statuses.key(2).to_s) %></td>
      <td><%= link_to 'Edit', edit_agent_path(agent) %>
      <%= link_to 'Delete', agent_path(agent), method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
